{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"airbnb-base",
		"plugin:cypress/recommended",
		"plugin:prettier/recommended"
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["cypress", "prettier"],
	"rules": {
		"no-console": "error",
		"prettier/prettier": "error"
	}
}
