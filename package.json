    "cypress-real-events": "^1.13.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "happy-dom": "^15.7.4",
    "knip": "^5.36.7",
